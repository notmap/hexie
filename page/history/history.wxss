.order {
	background-color: white;
	margin-bottom: 20rpx;
}

.order .head {
	height: 80rpx;
	padding: 14rpx 30rpx;
}

.order .head .logo {
	width: 80rpx;
	height: 80rpx;
	display: block;
	float: left;
	border-radius: 100%;
}

.order .head .name {
	display: block;
	float: left;
	height: 80rpx;
	line-height: 80rpx;
	/*background-color: blue;*/
	font-size: 28rpx;
	margin-left: 20rpx;
}

.order .head .status {
	display: block;
	float: right;
	height: 80rpx;
	line-height: 80rpx;
	/*background-color: red;*/
	font-size: 28rpx;
	/*margin-right: 20rpx;*/
}

.order .main {
	/*background-color: blue;*/
	/*padding: 20rpx;*/
	/*height: 60rpx;*/
}

.order .main .list {
	height: 40rpx;
	padding: 10rpx 30rpx 10rpx 130rpx;
	/*background-color: red;*/
	/*margin-bottom: 10rpx;*/
}

.order .main .list .good {
	display: block;
	float: left;
	height: 40rpx;
	line-height: 40rpx;
	font-size: 28rpx;
}

.order .main .list .amount {
	display: block;
	float: right;
	height: 40rpx;
	line-height: 40rpx;
	font-size: 28rpx;
	text-align: right;
}

.order .main .summary {
	height: 40rpx;
	padding: 10rpx 30rpx 10rpx 130rpx;
	/*background-color: red;*/
}

.order .main .summary .fold {
	display: block;
	float: left;
	height: 40rpx;
	line-height: 40rpx;
	font-size: 28rpx;
	/*background-color: yellow;*/
}

.order .main .summary .info {
	display: block;
	float: right;
	height: 40rpx;
	line-height: 40rpx;
	font-size: 28rpx;
	text-align: right;
	/*background-color: red;*/
}

.order .bottom {
	/*background-color: red;*/
	padding: 20rpx;
	height: 60rpx;
}

.order .bottom .button {
	height: 60rpx;
	font-size: 28rpx;
	border-radius: 8%;
	float: right;
	background-color: white;
	border: 2rpx solid #ddd;
	line-height: 60rpx;
	padding: 0 20rpx;
}
